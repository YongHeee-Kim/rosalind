    "import math\n",
    "\n",
    "k=\n",
    "n=\n",
    "\n",
    "prob = 0\n",
    "for i in range(n, k+1):\n",
    "    prob += math.comb(k, i) * (0.25**i) * (0.75**(k - i))\n",
    "\n",
    "print(f\"{prob:.3f}\")"
   ]
